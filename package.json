{
  "name": "electron-eddystone-beacon-scanner",
  "productName": "Eddystone beacon scanner",
  "version": "0.2.0",
  "description": "Eddystone beacon scanner for Mac Tray",
  "os": [
    "darwin"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": "ragingwind/electron-eddystone-beacon-scanner",
  "author": {
    "name": "ragingwind",
    "email": "ragingwind@gmail.com",
    "url": "ragingwind.me"
  },
  "engines": {
    "iojs": ">=2.0.0"
  },
  "scripts": {
    "start": "electron .",
    "clean": "rm -rf build; mkdir -p build",
